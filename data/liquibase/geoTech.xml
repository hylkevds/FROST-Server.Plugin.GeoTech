    <changeSet author="IOSB" id="2023-05-23_THINGS_view" objectQuotingStrategy="QUOTE_ALL_OBJECTS">
        <dropForeignKeyConstraint baseTableName="DATASTREAMS" constraintName="DATASTREAMS_THING_ID_FKEY" />
